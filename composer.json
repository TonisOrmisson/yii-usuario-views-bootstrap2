{
    "name": "usuario/yii-usuario-views-bootstrap3",
    "description": "Bootstrap 3 views for 2amigos/yii-usuario",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "keywords": [
        "yii",
        "yii2",
        "usuario",
        "yii2-usuario"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "2amigos/yii2-selectize-widget": "^1.1",
        "yiisoft/yii2-bootstrap": "^2.0"
    },
    "require-dev": {
        "php": ">=7.4",
        "friendsofphp/php-cs-fixer": "^3",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^1.8"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "auto"
        },
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
